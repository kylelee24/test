{
  "containerDefinitions":[
    {
      "name":"radar-api",
      "image":"643407245524.dkr.ecr.ca-central-1.amazonaws.com/radar-api:7155767bdc6836d61ee122b29fed3e182b23e91b",
      "cpu":0,
      "portMappings":[
        {
          "hostPort":3000,
          "protocol":"tcp",
          "containerPort":3000
        }
      ],
      "essential":true,
      "environment":[
        {
          "name":"PORT",
          "value":"3000"
        }
      ],
      "mountPoints":[],
      "volumesFrom":[],
      "logConfiguration":{
        "logDriver":"awslogs",
        "options":{
          "awslogs-group":"/ecs/radar-api",
          "awslogs-region":"ca-central-1",
          "awslogs-stream-prefix":"ecs"
        }
      },
      "healthCheck":{
        "command":[
          "CMD-SHELL",
          "curl -f http://localhost:3000/healthcheck || exit 1"
        ],
        "interval":30,
        "timeout":5,
        "retries":3
      },
      "secrets":[
        {
          "valueFrom":"arn:aws:secretsmanager:ca-central-1:643407245524:secret:prod/radar-api/database_url-ygZGcu",
          "name":"DATABASE_URL"
        }
      ]
    }
  ],
  "family":"radar-api",
  "executionRoleArn":"arn:aws:iam::643407245524:role/ecsTaskExecutionRole",
  "networkMode":"awsvpc",
  "volumes":[],
  "placementConstraints":[],
  "requiresCompatibilities":[
    "FARGATE"
  ],
  "cpu":"256",
  "memory":"512"
}